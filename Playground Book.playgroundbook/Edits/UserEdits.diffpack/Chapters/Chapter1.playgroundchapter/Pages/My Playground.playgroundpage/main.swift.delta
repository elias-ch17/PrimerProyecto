    var image: UIImage!
    var imageView: UIImageView!
    
    override func loadView() {
    
        let view = UIView(frame: CGRect(x:50, y:50, width: 100, height: 100))
        view.backgroundColor = .black
        
        textField = UITextField(frame: CGRect(x:200, y:400, width:300, height:50))
